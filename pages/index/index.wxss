/**index.wxss**/
page,navigator{
  box-sizing: border-box;
}
.swiper{
  width: 750rpx;
  height: 340rpx;
}
.swiper image{
  width: 100%;
}
.tab-nav-index{
  display: flex;
}
.tab-nav-index navigator{
  flex: 1;
  padding: 10rpx;
}
.tab-nav-index navigator image{
  width: 100%;
}
.floor-index{
  width: 100%;
}
.floor-index .floor-item .title{
  width: 100%;
  padding: 40rpx 0 10rpx 10rpx;
}
.floor-index .floor-item .floor-list{
  overflow: hidden;
  box-sizing: border-box;
}
.floor-index .floor-item .floor-list navigator{
  float: left;
  width: 33.333%;
}
.floor-index .floor-item .floor-list navigator:nth-last-child(-n+4) .img-item{
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.floor-index .floor-item .floor-list navigator:nth-child(2) image{
  border-bottom: 10rpx solid #fff;
}
.floor-index .floor-item .floor-list navigator:nth-child(3) image{
  border-bottom: 10rpx solid #fff;
}
.floor-index .floor-item .floor-list navigator image{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}